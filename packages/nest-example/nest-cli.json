{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nest-example/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "webpackConfigPath": "webpack.config.js",
    "tsConfigPath": "apps/nest-example/tsconfig.app.json"
  },
  "projects": {
    "nest-lib-example": {
      "type": "library",
      "root": "libs/nest-lib-example",
      "entryFile": "index",
      "sourceRoot": "libs/nest-lib-example/src",
      "compilerOptions": {
        "tsConfigPath": "libs/nest-lib-example/tsconfig.lib.json"
      }
    },
    "nest-example": {
      "type": "application",
      "root": "apps/nest-example",
      "entryFile": "main",
      "sourceRoot": "apps/nest-example/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest-example/tsconfig.app.json"
      }
    },
    "nest-app-example": {
      "type": "application",
      "root": "apps/nest-app-example",
      "entryFile": "main",
      "sourceRoot": "apps/nest-app-example/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest-app-example/tsconfig.app.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/nest-example"
}
